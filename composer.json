{
    "name": "escapework/laravel-uploader",
    "description": "Easy file upload for Laravel",
    "keywords": ["framework", "laravel", "uploader"],
    "license": "MIT",
    "type": "library",

    "autoload": {
        "psr-4": {
            "EscapeWork\\LaravelUploader\\": "src/"
        }
    },

    "require-dev": {
        "illuminate/support": "~5.0",
        "phpspec/phpspec": "~2.0"
    },

    "config": {
        "preferred-install": "stable"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.*-dev"
        }
    }
}
